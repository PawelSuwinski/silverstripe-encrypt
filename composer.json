{
  "name": "lekoala/silverstripe-encrypt",
  "description": "Encrypted database fields and files for SilverStripe",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "encrypt",
    "ciphersweet",
    "security",
    "decrypt",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "LeKoala",
      "email": "thomas@lekoala.be"
    }
  ],
  "require": {
    "php": ">7.2",
    "silverstripe/framework": "^4.4",
    "silverstripe/assets": "^1.6",
    "silverstripe/versioned": "^1.6",
    "paragonie/ciphersweet": "^3.0"
  },
  "require-dev": {
    "sminnee/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "LeKoala\\Encrypt\\": "src/",
      "LeKoala\\Encrypt\\Test\\": "tests/"
    }
  },
  "extra": {
    "installer-name": "encrypt"
  },
  "support": {
    "issues": "https://github.com/lekoala/silverstripe-encrypt/issues"
  },
  "scripts": {
    "lint": "phpcs src/ tests/",
    "lint-clean": "phpcbf src/ tests/",
    "test": "phpunit -v"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
